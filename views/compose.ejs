<%- include("partials/header"); -%>
<h1 class="display-6">Compose</h1>


<form class="" action="/compose" method="post">
    <div class="mb-3">
        <label for="titleForm" class="form-label">Title</label>
        <input type="text" class="form-control"  name="postTitle" autocomplete="off">
    </div>
    <div class="mb-3">
        <label for="postForm" class="form-label">Post</label>
        <textarea class="form-control" name="postBody" rows="4"></textarea>
    </div>
    <button class="btn btn-primary" type="submit" name="button1">Publish</button>
</form>

<%- include("partials/footer"); -%>